{
  "name": "node-producer",
  "version": "0.0.0",
  "description": "Kafka Producer",
  "devDependencies": {
    "@types/cors": "^2.8.11",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/jest": "~26.0.23",
    "@types/node": "~14.14.45",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "jest": "~27.0.6",
    "prettier": "~2.3.2",
    "rimraf": "~3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "~27.0.3",
    "tslint": "^6.1.3",
    "tsutils": "~3.21.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "compile": "tsc -p tsconfig.release.json && node build/src/main.js",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "./node_modules/nodemon/bin/nodemon.js --watch ./src --exec \"yarn build && yarn start\"  --verbose -e ts "
  },
