    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "kafka-node": "^5.0.0",
    "nodemon": "^2.0.10",
    "socket.io": "^4.1.2",
    "tslib": "~2.3.0",
    "winston": "^3.3.3"
  }
